, и разместить созданный объект на экране."}],"type":"paragraph"},{"syntax":"swift","code":["class ViewController: UIViewController {","","    override func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"],"type":"codeListing"},{"level":4,"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","text":"Использование в SwiftUI-проекте","type":"heading"},{"inlineContent":[{"text":"Для использования карты в SwiftUI необходимо создать объект ","type":"text"},{"code":"MapView","type":"codeVoice"},{"text":" и передать его в структуру ","type":"text"},{"code":"Map","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"type":"codeListing"}]}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapsnativesdk\/startusing"]}],"references":{"addedFrameworks":{"type":"image","identifier":"addedFrameworks","alt":"Added frameworks","variants":[{"traits":["1x","light"],"url":"\/images\/MapsNativeSDK\/addedFrameworks.png"}]},"https://dev.vk.com/ru/vkmaps/general-information/api-key":{"identifier":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","type":"link","titleInlineContent":[{"text":"портале","type":"text"}],"url":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","title":"портале"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapViewDelegate":{"role":"symbol","type":"topic","url":"\/documentation\/mapsnativesdk\/mapviewdelegate","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MapViewDelegate","kind":"identifier"}],"abstract":[{"text":"Interface, that MapView uses to inform its delegate about events on map","type":"text"}],"kind":"symbol","navigatorTitle":[{"text":"MapViewDelegate","kind":"identifier"}],"title":"MapViewDelegate","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapViewDelegate"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/Styles":{"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/