class ViewController: UIViewController {","","    override func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"]},{"text":"Использование в SwiftUI-проекте","type":"heading","level":4,"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5"},{"type":"paragraph","inlineContent":[{"text":"Для использования карты в SwiftUI необходимо создать объект ","type":"text"},{"code":"MapView","type":"codeVoice"},{"text":" и передать его в структуру ","type":"text"},{"code":"Map","type":"codeVoice"},{"text":".","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"]}],"kind":"content"}],"variants":[{"paths":["\/documentation\/mapsnativesdk\/startusing"],"traits":[{"interfaceLanguage":"swift"}]}],"hierarchy":{"paths":[["doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK"]]},"kind":"article","references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapConfiguration":{"role":"symbol","url":"\/documentation\/mapsnativesdk\/mapconfiguration","type":"topic","kind":"symbol","title":"MapConfiguration","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"MapConfiguration"}],"navigatorTitle":[{"kind":"identifier","text":"MapConfiguration"}],"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapConfiguration","abstract":[{"type":"text","text":"Configuration of Map"}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK/Lifecycle":{"title":"Отслеживание событий карты","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/Lifecycle","role":"article","kind":"article","type":"topic","url":"\/documentation\/mapsnativesdk\/lifecycle","abstract":[{"text":"Отслеживайте события, происходящие с картой, и программируйте реакцию на них.","type":"text"}]},"https://dev.vk.com/ru/vkmaps/general-information/api-key":{"title":"портале","type":"link","identifier":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","titleInlineContent":[{"text":"портале","type":"text"}],"url":"https:\/\/dev.vk.com\/ru\/vk