uration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"type":"codeListing","syntax":"swift"}]}],"metadata":{"role":"article","title":"Начало работы с MapsNativeSDK","roleHeading":"Article","modules":[{"name":"MapsNativeSDK"}]},"references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK":{"role":"collection","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK","title":"MapsNativeSDK","type":"topic","url":"\/documentation\/mapsnativesdk","kind":"symbol","abstract":[{"type":"strong","inlineContent":[{"type":"text","text":"MapsNativeSDK"}]},{"type":"text","text":" - библиотека, позволяющая использовать картографические данные и геосервисы VK в ваших iOS-приложениях."}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapView":{"fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"MapView"}],"abstract":[{"text":"Main object of the Map","type":"text"}],"url":"\/documentation\/mapsnativesdk\/mapview","kind":"symbol","type":"topic","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","role":"symbol","navigatorTitle":[{"text":"MapView","kind":"identifier"}],"title":"MapView"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapStyle":{"type":"topic","kind":"symbol","abstract":[],"url":"\/documentation\/mapsnativesdk\/mapstyle","navigatorTitle":[{"text":"MapStyle","kind":"identifier"}],"role":"symbol","fragments":[{"text":"stru