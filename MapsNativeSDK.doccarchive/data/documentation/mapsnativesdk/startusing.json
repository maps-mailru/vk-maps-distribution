aragraph"},{"syntax":"swift","code":["class ViewController: UIViewController {","","    override func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"],"type":"codeListing"},{"level":4,"text":"Использование в SwiftUI-проекте","anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Для использования карты в SwiftUI необходимо создать объект ","type":"text"},{"code":"MapView","type":"codeVoice"},{"text":" и передать его в структуру ","type":"text"},{"code":"Map","type":"codeVoice"},{"text":".","type":"text"}]},{"syntax":"swift","type":"codeListing","code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"]}]}],"sections":[],"abstract":[{"text":"Интегрируйте карты в свое приложение на основе UIKit или SwiftUI.","type":"text"}],"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapConfiguration":{"title":"MapConfiguration","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapConfiguration","navigatorTitle":[{"text":"MapConfiguration","kind":"identifier"}],"type":"topic","url":"\/documentation\/mapsnativesdk\/mapconfiguration","abstract":[{"type":"text","text":"Configuration of Map"}],"kind":"symbol","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MapConfiguration","kind":"identifier"}]},"https://dev.vk.com/ru/vkmaps/general-information/api-key":{"titleInlineContent":[{"type":"text","text":"портале"}],"title":"портале","type":"link","identifier":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","url":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapViewDelegate":{"role":"symbol","url":"\/documentation\/mapsnativesdk\/mapviewdelegate","title":"MapViewDelegate","navigatorTitle":[{"kind":"identifier","text":"MapViewDelegate"}],"kind":"symbol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Ma