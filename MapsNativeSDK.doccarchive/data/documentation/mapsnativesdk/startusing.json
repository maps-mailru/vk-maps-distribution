) {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"],"type":"codeListing"},{"type":"heading","text":"Использование в SwiftUI-проекте","level":4,"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5"},{"type":"paragraph","inlineContent":[{"text":"Для использования карты в SwiftUI необходимо создать объект ","type":"text"},{"code":"MapView","type":"codeVoice"},{"text":" и передать его в структуру ","type":"text"},{"code":"Map","type":"codeVoice"},{"text":".","type":"text"}]},{"syntax":"swift","code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"],"type":"codeListing"}],"kind":"content"}],"sections":[],"hierarchy":{"paths":[["doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK"]]},"abstract":[{"text":"Интегрируйте карты в свое приложение на основе UIKit или SwiftUI.","type":"text"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"article","references":{"doc://MapsNativeSDK/documentation/MapsNativeSDK/Styles":{"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/Styles","title":"Оформление карты","kind":"article","type":"topic","abstract":[{"type":"text","text":"Изменяйте внешний вид карты, рисуйте геометрические фигуры и размещайте маркеры."}],"role":"article","url":"\/documentation\/mapsnativesdk\/styles"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapView":{"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MapView","kind":"identifier"}],"title":"MapView","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","type":"topic","kind":"symbol","url":"\/documentation\/mapsnativesdk\/mapview","role":"symbol","navigatorTitle":[{"text":"MapView","kind":"identifier"}],"abstract":[{"text":"Main object of the Map","type":"text"}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapStyle":{"url":"\/documentation\/mapsnativesdk\/mapstyle","title":"MapStyle","kind":"symbol","role":"symbol","abstract":[],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MapStyle","kind":"identifier"}],"navigatorTitle":[{"text":"MapStyle","ki