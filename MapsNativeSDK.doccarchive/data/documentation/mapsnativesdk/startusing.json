ing","syntax":"swift","code":["\/\/ Определение класса-делегата","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ ...","","\/\/ Создание делегата","let delegate = BaseMapViewDelegate()"]},{"inlineContent":[{"type":"text","text":"В дальнейшем данный объект потребуется передать при создании экземпляра типа "},{"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","type":"reference","isActive":true},{"type":"text","text":"."}],"type":"paragraph"},{"type":"aside","name":"Note","content":[{"inlineContent":[{"type":"text","text":"Не требуется реализовывать методы "},{"type":"reference","isActive":true,"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapViewDelegate"},{"type":"text","text":", если их функциональность вам не нужна."}],"type":"paragraph"}],"style":"note"},{"anchor":"%D0%A0%D0%B0%D0%B7%D0%BC%D0%B5%D1%89%D0%B5%D0%BD%D0%B8%D0%B5-%D0%BA%D0%B0%D1%80%D1%82-%D0%BD%D0%B0-%D1%8D%D0%BA%D1%80%D0%B0%D0%BD%D0%B5","type":"heading","level":3,"text":"Размещение карт на экране"},{"inlineContent":[{"text":"MapsNativeSDK могут быть использованы в приложениях, основанных на UIKit и SwiftUI. Далее рассмотрим каждую из платформ.","type":"text"}],"type":"paragraph"},{"type":"aside","name":"Warning","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Убедитесь, что к этому моменту вы сконфигурировали SDK с помощью вызова "},{"type":"codeVoice","code":"MapsSDKConfigurator.setup"},{"type":"text","text":"."}]}],"style":"warning"},{"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-UIKit-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","type":"heading","level":4,"text":"Использование в UIKit-проекте"},{"inlineContent":[{"type":"text","text":"Для использования карты в UIKit необходимо создать объект "},{"identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","type":"reference","isActive":true},{"type":"text","text":", передав в него конфигурацию, и разместить созданный объект на экране."}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["class ViewController: UIViewController {","","    override func viewDidLoad() {","        super.viewDidLoad()","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание объекта карты","        let mapView = MapView(frame: CGRect(x: 0, y: 0,","                                            width: UIScreen.main.bounds.width,","                                            height: UIScreen.main.bounds.height),","                              configuration: configuration,","                              delegate: self)","        \/\/ Размещение карты на экране","        view.addSubview(mapView)","    }","}","","\/\/ Делегат карты","extension ViewController: MapViewDelegate {}"]},{"anchor":"%D0%98%D1%81%D0%BF%D0%BE%D0%BB%D1%8C%D0%B7%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5-%D0%B2-SwiftUI-%D0%BF%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D0%B5","type":"heading","level":4,"text":"Использование в SwiftUI-проекте"},{"inlineContent":[{"text":"Для использования карты в SwiftUI необходимо создать объект ","type":"text"},{"code":"MapView","type":"codeVoice"},{"text":" и передать его в структуру ","type":"text"},{"code":"Map","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["\/\/ Делегат для MapView","class BaseMapViewDelegate: MapViewDelegate {}","","\/\/ View-модель экрана, где будет использоваться карта","final class ContentViewModel: ObservableObject {","    \/\/ Объект карты","    let mapView: MapView","    \/\/ Объект делегата карты","    private let mapViewDelegate: MapViewDelegate","    ","    init() {","        \/\/ Конфигурирование SDK","        MapsSDKConfigurator.setup(apiKey: \"...\")","        \/\/ Создание конфигурации карты","        let configuration = MapConfiguration(center: Coordinates(lng: 37.6156, lat: 55.7522), zoom: 10, predefinedStyle: .main)","        \/\/ Создание делегата карты","        let delegate = BaseMapViewDelegate()","        mapViewDelegate = delegate","        \/\/ Создание объекта карты","        mapView = MapView(configuration: configuration, delegate: delegate)","    }","}","","\/\/ Экран с картой","struct ContentView: View {","    @StateObject private var viewModel = ContentViewModel()","","    var body: some View {","        Map(view: viewModel.mapView)","            .frame(maxWidth: .infinity, maxHeight: .infinity)","    }","}"]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK"]]},"references":{"addedFrameworks":{"identifier":"addedFrameworks","type":"image","alt":"Added frameworks","variants":[{"url":"\/images\/MapsNativeSDK\/addedFrameworks.png","traits":["1x","light"]}]},"https://dev.vk.com/ru/vkmaps/general-information/api-key":{"identifier":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","type":"link","url":"https:\/\/dev.vk.com\/ru\/vkmaps\/general-information\/api-key","title":"портале","titleInlineContent":[{"type":"text","text":"портале"}]},"doc://MapsNativeSDK/documentation/MapsNativeSDK/MapView":{"title":"MapView","identifier":"doc:\/\/MapsNativeSDK\/documentation\/MapsNativeSDK\/MapView","navigatorTitle":[{"kind":"identifier","text":"MapView"}],"kind":"symbol","url":"\/documentation\/mapsnativesdk\/mapview","role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"MapView"}],"abstract":[{"type":"text","text":"Main object of the Map"}],"type":"topic"},"doc://MapsNativeSDK/documentation/MapsNativeSDK/Lifecycle":{"title":"Отслеживание событий карты","type":"topic","abstract":[{"text":"Отслеживайте события, происходящие с картой, и программируйте реакцию на них.","type":"text